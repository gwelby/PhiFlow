node_modules/

# Ignore tests directory
/tests/

# Ignore test_quantum_backup directory
test_quantum_backup/

# Ignore build artifacts
build/
obj/

# Ignore any temporary files
*.tmp
*.temp

# Ignore all hidden files and directories
.*
